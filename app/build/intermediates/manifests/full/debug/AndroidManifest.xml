<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.behappy.hchen.androidlearningpath"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="25" />

    <uses-permission android:name="android.permission.INTERNET" />

    <meta-data
        android:name="android.support.VERSION"
        android:value="25.3.0" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.behappy.hchen.androidlearningpath.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.behappy.hchen.androidlearningpath.buttonclickcounter.ButtonClickCounter"
            android:label="@string/title_activity_button_click_counter"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.behappy.hchen.androidlearningpath.calculator.Calculator" />
        <activity android:name="com.behappy.hchen.androidlearningpath.top10downloader.Top10Downloader" />
        <activity
            android:name="com.behappy.hchen.androidlearningpath.reactiveandroid.ReactiveMain"
            android:label="@string/title_activity_reactive_main"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.behappy.hchen.androidlearningpath.youtubeplayer.YoutubeActivity" />
    </application>

</manifest>